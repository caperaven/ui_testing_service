{
    "id": "extension",
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "https://onkeydev.pragmaproducts.com/demo/daily?AuthenticationProviderName=Development Azure Account/#dashboard/AssetTree"
                },
                "next_step": "open_tree_search",
                "name": "start"
            },
            "open_tree_search": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "#ibtn-Search"
                },
                "next_step": "just_wait",
                "name": "open_tree_search"
            },
            "just_wait": {
                "type": "wait",
                "action": "time",
                "args": {
                    "timeout": 3
                },
                "next_step": "find_element",
                "name": "just_wait"
            },
            "find_element": {
                "type": "asset-tree",
                "action": "locate_element",
                "args": {
                    "query": "tree-search",
                    "element_id": 1707726914300001,
                    "element_type": "RegularAsset",
                    "data_id_value": "RegularAsset_1707726914300001"
                },
                "next_step": "step_4",
                "name": "find_element"
            },
            "step_4": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "[data-id='$p{found_element}']"
                },
                "next_step": "open_tree_search_again",
                "name": "step_4"
            },
            "open_tree_search_again": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "#ibtn-Search"
                },
                "next_step": "step_5"
            },
            "step_5": {
                "type": "asset-tree",
                "action": "locate_element",
                "args": {
                    "query": "tree-search",
                    "element_id": 3188,
                    "element_type": "RegularAssetComponent"
                },
                "next_step": "step_6"
            },
            "step_6": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "[data-id='$p{found_element}']"
                }
            }
        }
    }
}