{
    "id": "extension",
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "https://onkeydev.pragmaproducts.com/demo/daily?AuthenticationProviderName=Development Azure Account/#dashboard/AssetTree"
                },
                "next_step": "just_wait",
                "name": "start"
            },
            "just_wait": {
                "type": "wait",
                "action": "time",
                "args": {
                    "timeout": 3
                },
                "next_step": "find_element",
                "name": "just_wait"
            },
            "find_element": {
                "type": "asset-tree",
                "action": "locate_element",
                "args": {
                    "query": "asset-tree",
                    "element_id": 1707726914300001,
                    "element_type": "RegularAsset",
                    "data_id_value": "RegularAsset_1707726914300001"
                },
                "next_step": "step_4",
                "name": "find_element"
            },
            "step_4": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "[data-id='RegularAsset_1707726914300001']"
                },
                "next_step": "open_tree_search_again",
                "name": "step_4"
            },
            "open_tree_search_again": {
                "type": "wait",
                "action": "time",
                "args": {
                    "timeout": 2
                },
                "next_step": "step_5",
                "name": "open_tree_search_again"
            },
            "step_5": {
                "type": "asset-tree",
                "action": "locate_element",
                "args": {
                    "query": "asset-tree",
                    "element_id": 3188,
                    "element_type": "RegularAssetComponent"
                },
                "next_step": "step_6",
                "name": "step_5"
            },
            "step_6": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "[data-id='RegularAssetComponent_3188']"
                },
                "name": "step_6"
            }
        }
    }
}