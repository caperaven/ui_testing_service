{
    "id": "",
    "sequences": [
        {
            "caption": "TC1: Test Dashboard Label Formatting",
            "process": "main"
        },
        {
            "caption": "TC2: Test alignment, tooltip and label for toolbar items",
            "process": "TC2"
        },
        {
            "caption": "TC3: Test the functionality of the toolbar items",
            "process": "TC3"
        },
        {
            "caption": "TC4: Test Asset Tree Expand And Collapse",
            "process": "TC4"
        }
    ],
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "https://onkeydev.pragmaproducts.com/contoso/daily/#dashboard/AssetTree"
                },
                "next_step": "step_1",
                "name": "start"
            },
            "step_1": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "#asset-tree",
                    "attr": "status",
                    "value": "ready"
                },
                "next_step": "step_2",
                "name": "step_1"
            },
            "step_2": {
                "type": "assert",
                "action": "text_content_eq",
                "args": {
                    "query": "#pb-1 h2",
                    "value": "Asset Tree",
                    "property": "textContent"
                },
                "next_step": "step_3",
                "name": "step_2"
            },
            "step_3": {
                "type": "assert",
                "action": "style_property_eq",
                "args": {
                    "query": "#pb-1 h2",
                    "property": "font-family",
                    "value": "Source Sans Pro"
                },
                "next_step": "step_4",
                "name": "step_3"
            }
        }
    },
    "TC2": {
        "steps": {
            "start": {
                "type": "system",
                "action": "template",
                "args": {
                    "step": "start",
                    "schema": "hover=#ibtn-Search",
                    "process": "",
                    "parameters": {
                        "hover": "#ibtn-Search",
                        "value": "Search"
                    }
                },
                "next_step": "step_1"
            },
            "step_1": {
                "type": "system",
                "action": "template",
                "args": {
                    "step": "step_1",
                    "schema": "hover=#ibtn-PropertiesPanel",
                    "process": "",
                    "parameters": {
                        "hover": "#ibtn-PropertiesPanel",
                        "value": "Properties Panel"
                    }
                },
                "next_step": "step_2"
            },
            "step_2": {
                "type": "system",
                "action": "template",
                "args": {
                    "step": "step_2",
                    "schema": "hover=#ibtn-Refresh",
                    "process": "",
                    "parameters": {
                        "hover": "#ibtn-Refresh",
                        "value": "Refresh"
                    }
                },
                "next_step": "step_3"
            },
            "step_3": {
                "type": "perform",
                "action": "click_sequence",
                "args": {
                    "sequence": [
                        "#pb-1",
                        ".tree-item[data-id='RegularAsset_5000001002']"
                    ]
                },
                "next_step": "step_4"
            },
            "step_4": {
                "type": "assert",
                "action": "attribute_eq",
                "args": {
                    "query": "#ibtn-Add",
                    "attr": "aria-hidden",
                    "value": "false"
                },
                "next_step": "step_5"
            },
            "step_5": {
                "type": "perform",
                "action": "hover_over_element",
                "args": {
                    "query": "#ibtn-Add"
                },
                "next_step": "step_6"
            },
            "step_6": {
                "type": "assert",
                "action": "style_property_eq",
                "args": {
                    "query": "#ibtn-Search",
                    "property": "width",
                    "value": "40px"
                },
                "next_step": "step_7"
            },
            "step_7": {
                "type": "assert",
                "action": "style_property_eq",
                "args": {
                    "query": "#ibtn-Add",
                    "property": "width",
                    "value": "40px"
                },
                "next_step": "step_8"
            },
            "step_8": {
                "type": "assert",
                "action": "style_property_eq",
                "args": {
                    "query": "#ibtn-PropertiesPanel",
                    "property": "width",
                    "value": "40px"
                },
                "next_step": "step_9"
            },
            "step_9": {
                "type": "assert",
                "action": "style_property_eq",
                "args": {
                    "query": "#ibtn-Refresh",
                    "property": "width",
                    "value": "40px"
                },
                "next_step": "step_10"
            },
            "step_10": {
                "type": "assert",
                "action": "style_property_eq",
                "args": {
                    "query": "#ibtn-Search",
                    "property": "height",
                    "value": "40px"
                },
                "next_step": "step_11"
            },
            "step_11": {
                "type": "assert",
                "action": "style_property_eq",
                "args": {
                    "query": "#ibtn-Add",
                    "property": "height",
                    "value": "40px"
                },
                "next_step": "step_12"
            },
            "step_12": {
                "type": "assert",
                "action": "style_property_eq",
                "args": {
                    "query": "#ibtn-PropertiesPanel",
                    "property": "height",
                    "value": "40px"
                },
                "next_step": "step_13"
            },
            "step_13": {
                "type": "assert",
                "action": "style_property_eq",
                "args": {
                    "query": "#ibtn-Refresh",
                    "property": "height",
                    "value": "40px"
                },
                "next_step": "step_14"
            }
        }
    },
    "TC3": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "#ibtn-Search"
                },
                "next_step": "step_1"
            },
            "step_1": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "#asset-tree-split-view",
                    "attr": "class",
                    "value": "visible"
                },
                "next_step": "step_2"
            },
            "step_2": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "#ibtn-close"
                },
                "next_step": "step_3"
            },
            "step_3": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "#ibtn-PropertiesPanel"
                },
                "next_step": "step_4"
            },
            "step_4": {
                "type": "wait",
                "action": "element",
                "args": {
                    "query": ""
                },
                "next_step": "step_5"
            },
            "step_5": {
                "type": "perform",
                "action": "click_sequence",
                "args": {
                    "sequence": [
                        ".properties-panel-wrapper",
                        ".close"
                    ]
                },
                "next_step": "step_6"
            },
            "step_6": {
                "type": "perform",
                "action": "click_sequence",
                "args": {
                    "sequence": [
                        "#pb-1",
                        ".tree-item[data-id='RegularAsset_5000001002']"
                    ]
                },
                "next_step": "step_7"
            },
            "step_7": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "#ibtn-Refresh"
                },
                "next_step": "step_8"
            },
            "step_8": {
                "type": "wait",
                "action": "element",
                "args": {
                    "query": ""
                },
                "next_step": "step_9"
            }
        }
    },
    "TC4": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "click_sequence",
                "args": {
                    "sequence": [
                        "[data-id",
                        "=",
                        "'RegularAsset_1']",
                        ".tree-expand-arrow-container"
                    ]
                },
                "next_step": "step_1"
            },
            "step_1": {
                "type": "assert",
                "action": "element_not_exists ",
                "args": {
                    "query": ".tree-item:not(:nth-child(1))"
                },
                "next_step": "step_2"
            },
            "step_2": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": ".tree-expand-arrow-container"
                },
                "next_step": "step_3"
            },
            "step_3": {
                "type": "assert",
                "action": "element_exists",
                "args": {
                    "query": ".tree-item:not(:nth-child(1))"
                },
                "next_step": "step_4"
            },
            "step_4": {
                "type": "perform",
                "action": "click_sequence",
                "args": {
                    "sequence": [
                        "[data-id",
                        "='RegularAsset_5000001854']",
                        ".tree-expand-arrow-container"
                    ]
                },
                "next_step": "step_5"
            },
            "step_5": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "[data-id",
                    "attr": "='RegularAsset_5000001854']",
                    "value": ".tree-expand-arrow-container"
                },
                "next_step": "step_6"
            },
            "step_6": {
                "type": "perform",
                "action": "click_sequence",
                "args": {
                    "sequence": [
                        "[data-id",
                        "='RegularAsset_5000001854']",
                        ".tree-expand-arrow-container"
                    ]
                },
                "next_step": "step_7"
            },
            "step_7": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "[data-id",
                    "attr": "='RegularAsset_5000001854']",
                    "value": ".tree-expand-arrow-container"
                }
            }
        }
    }
}