{
    "id": "extension",
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "https://onkeydev.pragmaproducts.com/demo/daily?AuthenticationProviderName=Development Azure Account/#dashboard/AssetTree"
                },
                "next_step": "open_tree_search",
                "name": "start"
            },
            "open_tree_search": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "#ibtn-Search"
                },
                "next_step": "just_wait",
                "name": "open_tree_search"
            },
            "just_wait": {
                "type": "asset-tree",
                "action": "test",
                "args": {
                    "greeting1": "NEW",
                    "greeting2": "TEST"
                },
                "next_step": "find_element",
                "name": "just_wait"
            },
            "find_element": {
                "type": "asset-tree",
                "action": "find_element",
                "args": {
                    "query": "tree-search"
                },
                "next_step": "type_greeting",
                "name": "find_element"
            },
            "type_greeting": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "#btn-menu"
                },
                "next_step": "step_3",
                "name": "type_greeting"
            },
            "step_3": {
                "type": "perform",
                "action": "type_text",
                "args": {
                    "query": "pr-menu input",
                    "value": "test"
                },
                "next_step": "step_4"
            },
            "step_4": {
                "type": "wait",
                "action": "time",
                "args": {
                    "timeout": 5
                }
            }
        }
    }
}