{
    "id": "extension",
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "https://onkeydev.pragmaproducts.com/demo/daily?AuthenticationProviderName=Development Azure Account/#dashboard/AssetTree"
                },
                "next_step": "open_tree_search",
                "name": "start"
            },
            "open_tree_search": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "#ibtn-Search"
                },
                "next_step": "just_wait",
                "name": "open_tree_search"
            },
            "just_wait": {
                "type": "wait",
                "action": "time",
                "args": {
                    "timeout": 3
                },
                "next_step": "find_element",
                "name": "just_wait"
            },
            "find_element": {
                "type": "asset-tree",
                "action": "find_element",
                "args": {
                    "query": "tree-search"
                },
                "next_step": "step_4",
                "name": "find_element"
            },
            "step_4": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "$p{found_element}"
                }
            }
        }
    }
}