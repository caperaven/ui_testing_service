{
    "id": "test_3",
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "https://onkeydev.pragmaproducts.com/contoso/daily/#dashboard/WorkOrder"
                },
                "next_step": "click_create_button",
                "name": "start"
            },
            "click_create_button": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "#create"
                },
                "next_step": "click_lookup",
                "name": "click_create_button"
            },
            "click_lookup": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "dynamic-create [id='model.assetCode'] #btnLookup"
                },
                "next_step": "select_first_lookup_record"
            },
            "select_first_lookup_record": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "pragma-lookup .grid-row"
                },
                "next_step": "save_cell_value_to_variable"
            },
            "save_cell_value_to_variable": {
                "type": "perform",
                "action": "properties_to_variables",
                "args": {
                    "pragma-lookup .grid-row [role='cell']:nth-child(1)": {
                        "textContent": "$d{assetCodeInputValue}"
                    }
                },
                "next_step": "click_accept"
            },
            "click_accept": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "pragma-action-dialog #accept"
                },
                "next_step": "assert_input_value"
            },
            "assert_input_value": {
                "type": "assert",
                "action": "value_eq",
                "args": {
                    "query": "dynamic-create [label='Asset'] #lbl-assetCode",
                    "value": "$d{assetCodeInputValue}"
                }
            }
        }
    }
}