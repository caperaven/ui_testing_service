{
    "id": "visual-dashboard-test",
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "https://onkeydev.pragmaproducts.com/contoso/daily/#dashboard/WorkOrder"
                },
                "next_step": "wait_for_dashboard_grid_to_be_ready",
                "name": "start"
            },
            "wait_for_dashboard_grid_to_be_ready": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "#dashboard-grid",
                    "attr": "status",
                    "value": "ready",
                    "timeout": 30
                },
                "next_step": "select_visual_dashboard",
                "name": "wait_for_dashboard_grid_to_be_ready"
            },
            "select_visual_dashboard": {
                "type": "perform",
                "action": "select_option",
                "args": {
                    "query": "[id='model.selectedView'] select",
                    "value": "visualDashboard"
                },
                "next_step": "widget_types_of_work_ready",
                "name": "select_visual_dashboard"
            },
            "widget_types_of_work_ready": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "pr-widget[title='Types Of Work']",
                    "attr": "aria-busy",
                    "value": "false"
                },
                "next_step": "widget_assets_ready",
                "name": "widget_types_of_work_ready"
            },
            "widget_assets_ready": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "pr-widget[title='Assets']",
                    "attr": "aria-busy",
                    "value": "false"
                },
                "next_step": "widget_staff_members_ready"
            },
            "widget_staff_members_ready": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "pr-widget[title='Staff Members']",
                    "attr": "aria-busy",
                    "value": "false"
                },
                "next_step": "widget_status_ready"
            },
            "widget_status_ready": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "pr-widget[title='Status']",
                    "attr": "aria-busy",
                    "value": "false"
                }
            }
        }
    }
}