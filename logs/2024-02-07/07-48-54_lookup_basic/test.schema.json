{
    "id": "lookup_basic",
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "https://onkeydev.pragmaproducts.com/contoso/daily/#dashboard/WorkOrder"
                },
                "next_step": "wait_for_dashboard_grid_to_be_ready",
                "name": "start"
            },
            "wait_for_dashboard_grid_to_be_ready": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "#dashboard-grid",
                    "attr": "status",
                    "value": "ready"
                },
                "next_step": "set_uuid_variables",
                "name": "wait_for_dashboard_grid_to_be_ready"
            },
            "set_uuid_variables": {
                "type": "perform",
                "action": "set_uuid_variables",
                "args": {
                    "variables": [
                        "$d{codeName}",
                        "$d{description}"
                    ]
                },
                "next_step": "create_work_order_process",
                "name": "set_uuid_variables"
            },
            "create_work_order_process": {
                "type": "system",
                "action": "template",
                "args": {
                    "schema": "workOrderCreate",
                    "process": "work_order_create",
                    "parameters": {
                        "create_btn_selector": "pragma-panel-bar #create",
                        "description_field_selector": "82e4ce71-2c5d-4565-a242-b6db38f303d1",
                        "work_required_field_selector": "82e4ce71-2c5d-4565-a242-b6db38f303d1",
                        "work_order_code": "e6ea8bb1-8a28-4d35-9edb-b4c3443cdcbf"
                    }
                },
                "next_step": "wait_for_work_order_to_be_created",
                "name": "create_work_order_process"
            },
            "wait_for_work_order_to_be_created": {
                "type": "wait",
                "action": "time",
                "args": {
                    "timeout": 1
                },
                "next_step": "filter_work_order_record",
                "name": "wait_for_work_order_to_be_created"
            },
            "filter_work_order_record": {
                "type": "system",
                "action": "template",
                "args": {
                    "schema": "filterRecord",
                    "process": "filter_for_record",
                    "parameters": {
                        "fetch_all_records_selector": "[for='dashboard-grid'] #prepared-fetch",
                        "filter_button_selector": ".dashboard pr-responsive-dropdown#rd-3 #search-button",
                        "filter_input_selector": ".dashboard pr-responsive-dropdown#rd-3 pragma-filter input[type='search']",
                        "filter_value": "e6ea8bb1-8a28-4d35-9edb-b4c3443cdcbf",
                        "internal_filter_btn_selector": ".dashboard pr-responsive-dropdown#rd-3 #filter"
                    }
                },
                "next_step": "select_first_wo_record",
                "name": "filter_work_order_record"
            },
            "select_first_wo_record": {
                "type": "perform",
                "action": "dbl_click",
                "args": {
                    "query": "pragma-advanced-grid#dashboard-grid .grid-row",
                    "timeout": 1
                },
                "next_step": "wait_for_lookup_button",
                "name": "select_first_wo_record"
            },
            "wait_for_lookup_button": {
                "type": "wait",
                "action": "element",
                "args": {
                    "query": "dynamic-update [id='model.assetCode']",
                    "timeout": 15
                },
                "next_step": "click_assetCode_lookup",
                "name": "wait_for_lookup_button"
            },
            "click_assetCode_lookup": {
                "type": "perform",
                "action": "click",
                "args": {
                    "query": "pragma-action-dialog.small [id='model.assetCode'] #btnLookup"
                },
                "next_step": "wait_for_lookup_grid",
                "name": "click_assetCode_lookup"
            },
            "wait_for_lookup_grid": {
                "type": "wait",
                "action": "element",
                "args": {
                    "query": "#assetCode-lookup",
                    "timeout": 30
                },
                "next_step": "wait_for_lookup_grid_to_be_ready",
                "name": "wait_for_lookup_grid"
            },
            "wait_for_lookup_grid_to_be_ready": {
                "type": "wait",
                "action": "attribute",
                "args": {
                    "query": "#assetCode-lookup",
                    "attr": "status",
                    "value": "ready",
                    "timeout": 30
                },
                "next_step": "select_first_lookup_record",
                "name": "wait_for_lookup_grid_to_be_ready"
            },
            "select_first_lookup_record": {
                "type": "perform",
                "action": "dbl_click",
                "args": {
                    "query": "pragma-lookup .grid-row"
                },
                "next_step": "save_cell_value_to_variable",
                "name": "select_first_lookup_record"
            },
            "save_cell_value_to_variable": {
                "type": "perform",
                "action": "properties_to_variables",
                "args": {
                    "dynamic-update #lbl-assetCode": {
                        "value": "$d{assetCodeInputValue}"
                    }
                },
                "next_step": "wait_for_asset_code_input",
                "name": "save_cell_value_to_variable"
            },
            "wait_for_asset_code_input": {
                "type": "wait",
                "action": "element",
                "args": {
                    "query": "pragma-action-dialog.small #lbl-assetCode",
                    "timeout": 30
                },
                "next_step": "assert_input_value",
                "name": "wait_for_asset_code_input"
            },
            "assert_input_value": {
                "type": "assert",
                "action": "value_eq",
                "args": {
                    "query": "dynamic-update #lbl-assetCode",
                    "value": "15118",
                    "property": "value"
                },
                "name": "assert_input_value"
            }
        }
    }
}